{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"]
    },
    "strictFunctionTypes": false
  },
  "include": ["src"],
  "exclude": ["src/scripts/jcanvas/tests"]
}
