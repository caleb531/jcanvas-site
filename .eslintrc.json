{
  "root": true,
  "extends": ["eslint:recommended", "prettier"],
  "env": {
    "browser": true,
    "es2020": true,
    "node": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "globals": {
    "$": true,
    "jQuery": true,
    "CodeMirror": true
  },
  "rules": {
    "no-unused-vars": "off"
  }
}
